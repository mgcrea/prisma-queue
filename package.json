{
  "name": "@mgcrea/prisma-queue",
  "version": "1.14.0",
  "author": "Olivier Louvignes <olivier@mgcrea.io>",
  "repository": "github:mgcrea/fastify-session-prisma-store",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=12"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "prisma",
    "postgresql",
    "job",
    "queue",
    "worker"
  ],
  "scripts": {
    "start": "DEBUG=prisma-queue,prisma-query vitest",
    "build": "tsup",
    "lint": "eslint src/ test/",
    "prettycheck": "prettier --check src/ test/",
    "format": "prettier --write src/ test/",
    "typecheck": "tsc --noEmit",
    "spec": "DEBUG=prisma-queue,prisma-query vitest --run --pool=forks",
    "coverage": "DEBUG=prisma-queue,prisma-query vitest --run --pool=forks --coverage",
    "dev": "DEBUG=prisma-queue vitest --watch --pool=forks --reporter=dot",
    "test": "pnpm run lint && pnpm run prettycheck && pnpm run typecheck && pnpm run spec",
    "prepare": "prisma generate",
    "reset": "prisma db push --force-reset && prisma generate",
    "prepublishOnly": "pnpm run build",
    "with-env": "dotenv -e ./.env --"
  },
  "dependencies": {
    "croner": "^9.1.0",
    "debug": "^4.4.3"
  },
  "peerDependencies": {
    "@prisma/client": ">=7"
  },
  "devDependencies": {
    "@mgcrea/eslint-config-node": "^0.14.1",
    "@prisma/adapter-pg": "^7.4.0",
    "@prisma/client": "^7.4.0",
    "@tsconfig/node-lts": "^24.0.0",
    "@tsconfig/strictest": "^2.0.8",
    "@types/debug": "^4.1.12",
    "@types/node": "^25.2.3",
    "@vitest/coverage-v8": "^4.0.18",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.2",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prisma": "^7.4.0",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^6.1.1",
    "vitest": "^4.0.18"
  },
  "packageManager": "pnpm@10.28.0"
}
